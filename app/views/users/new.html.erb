<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= render 'user_form' %>

<%= link_to "Login", login_path(@session) %>

<% if @user.errors.full_messages.any? %>
	<ul>
		<% @user.errors.full_messages.each do |error_message| %>
			<li><%= error_message %></li> <br />
		<% end %>
	</ul>
<% end %>