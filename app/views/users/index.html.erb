<%= link_to "Go Back to Profile", user_path(@user) %> <!-- returns user back to his/her profile -->
<br>
<% @users.each do |user| %>
	<%= user.fname %> <%= user.lname %>
	<%= form_for Friendship.new do |f| %>
		<%= hidden_field_tag('friend_id', user.id) %>
		<%= f.submit %>
	<% end %>
	<br>
<% end %>
	