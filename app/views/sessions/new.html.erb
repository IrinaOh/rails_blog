<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_tag("/login", method: "post") do %>   
	<%= label_tag(:email, "Email") %>   
	<%= email_field_tag(:email) %>  
	<%= label_tag(:password, "Password") %>   
	<%= password_field_tag(:password) %>   
	<%= submit_tag("Login") %> 
<% end %>